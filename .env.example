# Resend API Key para el envío de emails
# Obtén tu clave en: https://resend.com/api-keys
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxxxxxxx

# Cloudflare Turnstile (SECRET_KEY es privada, SITE_KEY es pública)
# Configura Turnstile en: https://dash.cloudflare.com/
TURNSTILE_SECRET_KEY=0x4AAAAAAxxxxxxxxxxxxxxxxxxxxxxxxx
PUBLIC_TURNSTILE_SITE_KEY=0x4AAAAAAxxxxxxxxxxxxxxxxxxxxxxxxx

# Email de destino para el formulario de contacto
CONTACT_EMAIL=tu_email@ejemplo.com

# Instagram API Configuration
# Cache duration in minutes (default: 30)
INSTAGRAM_CACHE_DURATION=30
# Rate limiting - requests per hour (default: 100)
INSTAGRAM_RATE_LIMIT=100
# Enable/disable Instagram scraping (default: true)
INSTAGRAM_ENABLED=true