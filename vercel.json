{
  "functions": {
    "src/pages/api/contact.ts": { "maxDuration": 10 },
    "src/pages/api/instagram.ts": { "maxDuration": 10 },
    "src/pages/api/instagram/auth.ts": { "maxDuration": 10 },
    "src/pages/api/instagram/callback.ts": { "maxDuration": 10 }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}